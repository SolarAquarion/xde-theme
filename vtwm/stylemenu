divert(FUNCTIONS)dnl
divert(MENUS)dnl
Menu "twmstyles" (twm_deffore:twm_defback)
{
    "System Styles"		f.title
    "User Styles"		f.title
divert(FUNCTIONS)dnl
Function "set-user-style-Airforce" { f.exec "/home/brian/.vtwm/setstyle TWM_TYPE /home/brian/.vtwm/looks/Airforce" f.function "reconfig" }
divert(MENUS)dnl
    "Airforce"		f.function "set-user-style-Airforce"
divert(FUNCTIONS)dnl
Function "set-user-style-Classic" { f.exec "/home/brian/.vtwm/setstyle TWM_TYPE /home/brian/.vtwm/looks/Classic" f.function "reconfig" }
divert(MENUS)dnl
    "Classic"		f.function "set-user-style-Classic"
divert(FUNCTIONS)dnl
Function "set-user-style-Squared-blue" { f.exec "/home/brian/.vtwm/setstyle TWM_TYPE /home/brian/.vtwm/looks/Squared-blue" f.function "reconfig" }
divert(MENUS)dnl
    "Squared-blue"		f.function "set-user-style-Squared-blue"
divert(FUNCTIONS)dnl
Function "set-user-style-Squared-cadet" { f.exec "/home/brian/.vtwm/setstyle TWM_TYPE /home/brian/.vtwm/looks/Squared-cadet" f.function "reconfig" }
divert(MENUS)dnl
    "Squared-cadet"		f.function "set-user-style-Squared-cadet"
divert(FUNCTIONS)dnl
Function "set-user-style-Squared-green" { f.exec "/home/brian/.vtwm/setstyle TWM_TYPE /home/brian/.vtwm/looks/Squared-green" f.function "reconfig" }
divert(MENUS)dnl
    "Squared-green"		f.function "set-user-style-Squared-green"
divert(FUNCTIONS)dnl
Function "set-user-style-Squared-grey" { f.exec "/home/brian/.vtwm/setstyle TWM_TYPE /home/brian/.vtwm/looks/Squared-grey" f.function "reconfig" }
divert(MENUS)dnl
    "Squared-grey"		f.function "set-user-style-Squared-grey"
}
